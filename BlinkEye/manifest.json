{
    "manifest_version": 3,
    "name": "Blink Reminder",
    "version": "1.0",
    "description": "A simple eye-blink reminder extension.",
    "permissions": ["alarms"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "icon": "icon16.png"
      }
    },
    "icons": {
      "icon": "icon16.png"
      
    },
    "background": {
      "service_worker": "background.js"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "icon": "icon16.png"
        
      }
    }
  }
  